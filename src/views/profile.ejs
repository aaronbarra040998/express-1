      <section class="join">
        
        <div class="join__container container">
          <h3 class="join__title">  Enroll in your favorite course.
          </h3>
          <div class="container_box">
  
            <form action="/confirmacion" method="post" class="styled-form">
                <div class="form-group row">
                    <label for="curso" class="col-sm-3 col-form-label text-right">
                        <h3>Curso:</h3>
                    </label>
                    <div class="com-box">
                        <select class="form-options" id="curso" name="curso">
                            <option value="Java">Java</option>
                            <option value="PHP">PHP</option>
                            <option value=".NET">.NET</option>
                        </select>
                    </div>
                </div>
  
                <h2>Seleccione los módulos que el participante llevará:</h2>
                <div class="form-check">
                    <input type="checkbox" class="form-check-input" id="basico" name="modulos[]" value="Basico">
                    <label class="form-check-label" for="basico">Básico</label>
                </div>
                <div class="form-check">
                    <input type="checkbox" class="form-check-input" id="intermedio" name="modulos[]" value="Intermedio">
                    <label class="form-check-label" for="intermedio">Intermedio</label>
                </div>
                <div class="form-check">
                    <input type="checkbox" class="form-check-input" id="avanzado" name="modulos[]" value="Avanzado">
                    <label class="form-check-label" for="avanzado">Avanzado</label>
                </div>
  
                <h2>Seleccione el medio de pago:</h2>
                <div class="form-check">
                    <input type="radio" class="form-check-input" id="tarjeta" name="medioPago"
                        value="Tarjeta de crédito">
                    <label class="form-check-label" for="tarjeta">Tarjeta de crédito</label>
                </div>
                <div class="form-check">
                    <input type="radio" class="form-check-input" id="efectivo" name="medioPago"
                        value="Pago en efectivo">
                    <label class="form-check-label" for="efectivo">Pago en efectivo</label>
                </div>
  
                <button type="submit" class="join__input join__input--submit">Siguiente</button>
                
            </form>
        </div>
        </div>
      </section>
